@model Parcial2_FlorezRSebastian.DAL.Entities.Ticket

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h3>Estas seguro que deseas eliminar el tiquete?</h3>
<div>

    <partial name="../Shared/PartialViews/_TicketDetails.cshtml" />
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar" class="btn btn-outline-danger" /> |
        <a asp-action="Index" class="btn btn-outline-success">Regresar</a>
    </form>
</div>
